{
  "expo": {
    "name": "sistem-desa-mob",
    "slug": "sistem-desa-mob",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.example.sistemdesamob"
    },
    "ios": {
      "bundleIdentifier": "com.example.sistemdesamob"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "react-native-edge-to-edge"
    ]
  }
}
